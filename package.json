{
    "name": "llm-key-manager",
    "version": "0.1.0",
    "type": "module",
    "main": "./dist/llm-key-manager.umd.js",
    "module": "./dist/llm-key-manager.es.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/llm-key-manager.es.js",
            "require": "./dist/llm-key-manager.umd.js"
        }
    },
    "scripts": {
        "dev": "cd demo && vite",
        "build:lib": "tsc && vite build --config vite.config.lib.ts",
        "build:demo": "cd demo && vite build",
        "build": "pnpm build:lib",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "test": "vitest run",
        "test:watch": "vitest",
        "preview": "cd demo && vite preview"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.71.2",
        "@google/generative-ai": "^0.24.1",
        "@types/uuid": "^10.0.0",
        "clsx": "^2.1.0",
        "dexie": "^3.2.4",
        "lucide-react": "^0.344.0",
        "openai": "^6.16.0",
        "tailwind-merge": "^2.2.1",
        "uuid": "^13.0.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@peculiar/webcrypto": "^1.5.0",
        "@types/node": "^25.0.10",
        "@types/react": "^18.2.64",
        "@types/react-dom": "^18.2.21",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@typescript-eslint/parser": "^8.54.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/coverage-v8": "^4.0.18",
        "autoprefixer": "^10.4.18",
        "eslint": "^9.39.2",
        "fake-indexeddb": "^6.2.5",
        "happy-dom": "^20.4.0",
        "postcss": "^8.4.35",
        "scheduler": "0.23.2",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.2.2",
        "vite": "^5.1.5",
        "vite-plugin-dts": "^3.9.1",
        "vitest": "^4.0.18"
    }
}